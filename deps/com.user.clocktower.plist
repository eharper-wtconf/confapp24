<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
    "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.user.clocktower</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Users/jmartinez/Applications/Clocktower Inc..app</string>
    </array>
    <key>StartInterval</key>
    <integer>21600</integer> <!-- 6 hours in seconds -->
    <key>StandardOutPath</key>
    <string>/tmp/com.user.clocktower.out</string>
    <key>StandardErrorPath</key>
    <string>/tmp/com.user.clocktower.err</string>
</dict>
</plist>
